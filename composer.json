{
    "name": "ownpass/ownpass",
    "description": "The OwnPass application providing an API and interface to manage credentials.",
    "type": "project",
    "license": "proprietary",
    "keywords": [
        "api",
        "password",
        "management",
        "own"
    ],
    "homepage": "http://github.com/ownpass",
    "support": {
        "irc": "irc://irc.freenode.net/ownpass",
        "source": "https://github.com/ownpass/ownpass",
        "issues": "https://github.com/ownpass/ownpass/issues"
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 5000,
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/doctrine-module": "dev-master",
        "doctrine/doctrine-orm-module": "dev-master",
        "gedmo/doctrine-extensions": "^2.4",
        "ramsey/uuid-doctrine": "^1.2",
        "swiftmailer/swiftmailer": "^5.4",
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-mvc-console": "^1.1",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-text": "^2.6",
        "zfcampus/zf-apigility": "^1.3",
        "zfcampus/zf-apigility-documentation": "^1.2.2",
        "zfcampus/zf-development-mode": "^3.0",
        "zfcampus/zf-http-cache": "^1.3",
        "zfr/zfr-cors": "^1.3"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "^1.1",
        "zendframework/zend-test": "^3.0",
        "zfcampus/zf-apigility-admin": "^1.5.6",
        "zfcampus/zf-asset-manager": "^1.0",
        "zfcampus/zf-composer-autoloading": "^1.0",
        "zfcampus/zf-deploy": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "OwnPassApplication\\": "module/OwnPassApplication/src/",
            "OwnPassCredential\\": "module/OwnPassCredential/src/",
            "OwnPassOAuth\\": "module/OwnPassOAuth/src/",
            "OwnPassNotes\\": "module/OwnPassNotes/src/",
            "OwnPassUser\\": "module/OwnPassUser/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwnPassApplicationTest\\": "module/OwnPassApplication/tests/",
            "OwnPassCredentialTest\\": "module/OwnPassCredential/tests/",
            "OwnPassNotesTest\\": "module/OwnPassNotes/tests/",
            "OwnPassOAuthTest\\": "module/OwnPassOAuth/tests/",
            "OwnPassUserTest\\": "module/OwnPassUser/tests/"
        },
        "files": [
            "src/Apigility/constants.php"
        ]
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
